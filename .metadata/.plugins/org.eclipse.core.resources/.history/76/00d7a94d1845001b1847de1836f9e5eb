  @Test
  public void wikipedia() {
    driver.get("https://sr.wikipedia.org/wiki/%D0%93%D0%BB%D0%B0%D0%B2%D0%BD%D0%B0_%D1%81%D1%82%D1%80%D0%B0%D0%BD%D0%B0");
    driver.manage().window().setSize(new Dimension(864, 695));
    {
      WebElement element = driver.findElement(By.linkText("Википедију"));
      Actions builder = new Actions(driver);
      builder.moveToElement(element).perform();
    }
    {
      WebElement element = driver.findElement(By.tagName("body"));
      Actions builder = new Actions(driver);
      builder.moveToElement(element, 0, 0).perform();
    }
    {
      WebElement element = driver.findElement(By.linkText("гласање за избор википедијанца 2020. године"));
      Actions builder = new Actions(driver);
      builder.moveToElement(element).perform();
    }
    driver.findElement(By.linkText("Садржај")).click();
    driver.findElement(By.cssSelector("#\\413\\435\\43E\\433\\440\\430\\444\\438\\458\\430 > a")).click();
    driver.findElement(By.linkText("Земља и људи")).click();
    driver.findElement(By.cssSelector("ul:nth-child(4) > li:nth-child(1) > a")).click();
    driver.findElement(By.cssSelector("ul:nth-child(11) > li:nth-child(1) > a")).click();
    driver.findElement(By.linkText("Кнежевина Србија")).click();
    driver.findElement(By.linkText("English")).click();
    {
      WebElement element = driver.findElement(By.cssSelector(".tocsection-12 > a > .toctext"));
      Actions builder = new Actions(driver);
      builder.moveToElement(element).perform();
    }
  }
